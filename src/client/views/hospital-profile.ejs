                <div class="each-feature" id="diff-feature">
                    <div>
                    <i class="fa fa-address-book fa-lg" aria-hidden="true"></i>
                    <h3>Specialization</h3>
                    </div>
                    <div>
                        <i class="fa fa-arrow-circle-up " id = "only-hide"  onclick="hidePara('show-p1','only-show','only-hide')" aria-hidden="true"></i>
                        <i class="fa fa-arrow-circle-down" id = "only-show" onclick="showPara('show-p1','only-show','only-hide')" aria-hidden="true"></i>

                    </div>
                </div>
                <p class="show-p" id="show-p1" >
                    <%= hospital.specialization %> is our Speciality.
                </p>
                </div>
                <div>
                    <div class="each-feature" id="diff-feature">
                        <div>
                        <i class="fa fa-stethoscope fa-lg" aria-hidden="true"></i>
                        <h3>Famous Treatments</h3>
                        </div>
                        <div>
                            <i class="fa fa-arrow-circle-up " id = "only-hide1"  onclick="hidePara('show-p2','only-show1','only-hide1')" aria-hidden="true"></i>
                            <i class="fa fa-arrow-circle-down" id = "only-show1" onclick="showPara('show-p2','only-show1','only-hide1')" aria-hidden="true"></i>
    
                        </div>
                    </div>
                    <p class="show-p" id="show-p2" >
                        <%= hospital.treatments %>+ SuccessFul Treatments. 
                    </p>
                </div>
                <div>
                    <div class="each-feature" id="diff-feature">
                        <div>
                        <i class="fa fa-user fa-lg" aria-hidden="true"></i>
                        <h3>Famous Doctors</h3>
                        </div>
                        <div>
                            <i class="fa fa-arrow-circle-up " id = "only-hide2"  onclick="hidePara('show-p3','only-show2','only-hide2')" aria-hidden="true"></i>
                            <i class="fa fa-arrow-circle-down" id = "only-show2" onclick="showPara('show-p3','only-show2','only-hide2')" aria-hidden="true"></i>
    
                        </div>
                    </div>
                    <p class="show-p" id="show-p3" >
                        <%= hospital.doctors %>+ Trusted Doctors.
                    </p>
                </div>
                <div>
                    <div class="each-feature" id="diff-feature">
                        <div>
                        <i class="fa fa-building fa-lg" aria-hidden="true"></i>
                        <h3>Infrastrucutre</h3>
                        </div>
                        <div>
                            <i class="fa fa-arrow-circle-up " id = "only-hide3"  onclick="hidePara('show-p4','only-show3','only-hide3')" aria-hidden="true"></i>
                            <i class="fa fa-arrow-circle-down" id = "only-show3" onclick="showPara('show-p4','only-show3','only-hide3')" aria-hidden="true"></i>
    
                        </div>
                    </div>
                    <p class="show-p" id="show-p4" >
                        <%= hospital.infrastructure %> infrastrucutre with awesome Campus.
                    </p>
                </div>
                <div>
                    <div class="each-feature" id="diff-feature">
                        <div>
                        <i class="fa fa-h-square fa-lg" aria-hidden="true"></i>
                        <h3>Beds</h3>
                        </div>
                        <div>
                            <i class="fa fa-arrow-circle-up " id = "only-hide4"  onclick="hidePara('show-p5','only-show4','only-hide4')" aria-hidden="true"></i>
                            <i class="fa fa-arrow-circle-down" id = "only-show4" onclick="showPara('show-p5','only-show4','only-hide4')" aria-hidden="true"></i>
    
                        </div>
                    </div>
                    <p class="show-p" id="show-p5" >
                        <%= hospital.beds %>+ beds.
                    </p>
                </div>
                <div>
                    <div class="each-feature" id="diff-feature">
                        <div>
                        <i class="fa fa-map-marker fa-lg" aria-hidden="true"></i>
                        <h3>Complete Address</h3>
                        </div>
                        <div>
                            <i class="fa fa-arrow-circle-up " id = "only-hide5"  onclick="hidePara('show-p6','only-show5','only-hide5')" aria-hidden="true"></i>
                            <i class="fa fa-arrow-circle-down" id = "only-show5" onclick="showPara('show-p6','only-show5','only-hide5')" aria-hidden="true"></i>
    
                        </div>
                    </div>
                    <p class="show-p" id="show-p6" >
                        <%= hospital.address %>
                    </p>
                </div>
                <div>
                    <div class="each-feature" id="diff-feature">
                        <div>
                        <i class="fa fa-usd fa-lg" aria-hidden="true"></i>
                        <h3>Hospital Chagres</h3>
                        </div>
                        <div>
                            <i class="fa fa-arrow-circle-up " id = "only-hide6"  onclick="hidePara('show-p7','only-show6','only-hide6')" aria-hidden="true"></i>
                            <i class="fa fa-arrow-circle-down" id = "only-show6" onclick="showPara('show-p7','only-show6','only-hide6')" aria-hidden="true"></i>
    
                        </div>
                    </div>
                    <p class="show-p" id="show-p7" >
                        <%= hospital.fees %>
                    </p>
                </div>
                



            </div>
        </div>
    </section>



    <section class="need-help" >
        <div>
        <h2>Need Help</h2>
        <p>Just let us know. We will be happy to assist you.</p>
        </div>
        <div>
        <button class="help-btn" >Submit your query</button>
        </div>
